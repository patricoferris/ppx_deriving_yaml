  age: int
    [@of_yaml
      function
      | `Float f -> Ok (int_of_float (f +. 10.))
      | _ -> Error (`Msg "Expected a Yaml `Float")]}[@@deriving yaml]
include
  struct
    let x_to_yaml (x : x) =
      `O
